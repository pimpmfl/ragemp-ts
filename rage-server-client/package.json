{
  "name": "ragemp-server-client",
  "version": "0.0.1",
  "description": "RAGE MP server with TypeScript",
  "scripts": {
    "preinstall": "node ../scripts/enforce-pnpm.js",
    "build": "rollup -c ./build-scripts/rollup.config.mjs --environment NODE_ENV:prod",
    "build:dev": "rollup -c ./build-scripts/rollup.config.mjs",
    "watch": "rollup -w -c ./build-scripts/rollup.config.mjs",
    "check-format": "prettier --check src/ --config ../.prettierrc",
    "lint": "eslint ."
  },
  "author": "pimpmfl",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.25.0",
    "@ragempcommunity/types-client": "2.1.9",
    "@ragempcommunity/types-server": "2.1.9",
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@swc/core": "1.11.21",
    "@types/node": "22.14.1",
    "dotenv": "16.5.0",
    "eslint": "9.25.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "minimatch": "10.0.1",
    "prettier": "3.5.3",
    "rollup": "4.40.0",
    "rollup-plugin-swc3": "0.12.1",
    "rollup-plugin-tsconfig-paths": "1.5.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1"
  },
  "engines": {
    "node": ">=22.14.0 <23",
    "pnpm": ">=10.8.1 <11"
  }
}
